# AirCast CLI Configuration
# Copy this file to .env and customize for your environment

# API URL - defaults to production (https://api.aircast.one)
# For local development, uncomment and set:
AIRCAST_API_URL=http://localhost:3333

# TCP listen address for MAVLink clients
AIRCAST_TCP_LISTEN=127.0.0.1:14550

# UDP listen address for MAVLink clients (optional)
# AIRCAST_UDP_LISTEN=127.0.0.1:14551

# Log level: trace, debug, info, warn, error
LOG_LEVEL=info
